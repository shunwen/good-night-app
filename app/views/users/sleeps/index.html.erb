<p style="color: green"><%= notice %></p>

<% content_for :title, "Sleeps" %>

<h1>Sleeps</h1>

<div id="sleeps">
  <% @sleeps.each do |sleep| %>
    <%= render partial: 'users/sleeps/sleep', locals: { sleep: } %>
    <p>
      <%= link_to "Show this sleep", users_sleep_path(sleep) %>
    </p>
  <% end %>
</div>

<%= link_to "New sleep", new_users_sleep_path %>
